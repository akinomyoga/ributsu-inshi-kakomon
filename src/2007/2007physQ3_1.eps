<< >> begin
%-------------------------------------------------------------------------------
%   矢印
%-------------------------------------------------------------------------------

/mwg-option-arrow-head-scale 1 def
/mwg-arrow-headA {
  0 0 moveto
  -10 -5 rlineto
  4 5 rlineto
  -4 5 rlineto
  closepath
  fill
} def

%: <x> <y> <dx> <dy> mwg-draw-rarrow
/mwg-draw-rarrow {
  gsave
    4 2 roll translate
    0 0 moveto
    2 copy rlineto
    stroke
    
    2 copy translate
    exch atan rotate
    mwg-option-arrow-head-scale dup scale
    mwg-arrow-headA
  grestore
} def

%: <x> <y> <X> <Y> mwg-draw-arrow
/mwg-draw-arrow {
  2 index sub
  exch 3 index sub exch
  mwg-draw-rarrow
} def

%-------------------------------------------------------------------------------
%   矩形
%-------------------------------------------------------------------------------
/mwg-path-rrect {
  newpath
  4 2 roll moveto
  1 index 0 rlineto
  0 exch rlineto
  neg 0 rlineto
  closepath
} def

/mwg-path-rect {
  2 index sub
  exch 3 index sub exch
  mwg-path-rrect
} def

/mwg-draw-rect  {mwg-path-rect  stroke} def
/mwg-draw-rrect {mwg-path-rrect stroke} def
/mwg-fill-rect  {mwg-path-rect  fill} def
/mwg-fill-rrect {mwg-path-rrect fill} def

% mwg-draw-rrect -> rectstroke
% mwg-fill-rrect -> rectfill
%-------------------------------------------------------------------------------
