/2007physQ4_2.save save def
<< >> begin
%-------------------------------------------------------------------------------
%   矢印
%-------------------------------------------------------------------------------

/mwg-option-arrow-head-scale 1 def
/mwg-arrow-headA {
  0 0 moveto
  -10 -5 rlineto
  4 5 rlineto
  -4 5 rlineto
  closepath
  fill
} def

%: <x> <y> <dx> <dy> mwg-draw-rarrow
/mwg-draw-rarrow {
  gsave
    4 2 roll translate
    0 0 moveto
    2 copy rlineto
    stroke
    
    2 copy translate
    exch atan rotate
    mwg-option-arrow-head-scale dup scale
    mwg-arrow-headA
  grestore
} def

%: <x> <y> <X> <Y> mwg-draw-arrow
/mwg-draw-arrow {
  2 index sub
  exch 3 index sub exch
  mwg-draw-rarrow
} def

%-------------------------------------------------------------------------------
%   矩形
%-------------------------------------------------------------------------------
/mwg-path-rrect {
  newpath
  4 2 roll moveto
  1 index 0 rlineto
  0 exch rlineto
  neg 0 rlineto
  closepath
} def

/mwg-path-rect {
  2 index sub
  exch 3 index sub exch
  mwg-path-rrect
} def

/mwg-draw-rect  {mwg-path-rect  stroke} def
/mwg-draw-rrect {mwg-path-rrect stroke} def
/mwg-fill-rect  {mwg-path-rect  fill} def
/mwg-fill-rrect {mwg-path-rrect fill} def

% mwg-draw-rrect -> rectstroke
% mwg-fill-rrect -> rectfill
%-------------------------------------------------------------------------------
%   フォント設定 / 文字列表示
%-------------------------------------------------------------------------------

/mwg-font-gothic {
  /GothicBBB-Medium-EUC-H findfont
  exch scalefont
  setfont
} def

/mwg-font-mincho {
  /Ryumin-Light-EUC-H findfont
  exch scalefont
  setfont
} def

/mwg-font-math {
  /Times-Italic-H findfont
  exch scalefont
  setfont
} def

/mwg-font-isym {
  /Symbol-H findfont
  [1 0 0.2 1 0 0] makefont
  exch scalefont
  setfont
} def

/mwg-font-sym {
  /Symbol-H findfont
  exch scalefont
  setfont
} def

/mwg-font-roman {
  /Times-Roman-H findfont
  exch scalefont
  setfont
} def

/mwg-font-helv {
  /Helvetica-H findfont
  exch scalefont
  setfont
} def

/mwg-center-show {
  dup stringwidth pop 2 div neg 0 rmoveto show
} def
%-------------------------------------------------------------------------------
%   波線
%-------------------------------------------------------------------------------

/mwg-wave-p1 {
  10 5 20 10 30 10 curveto
  30  0 translate
} def

/mwg-wave-p2 {
  10 10 20 5 30 0 curveto
  30  0 translate
} def

/mwg-wave-p3 {
  10 -5 20 -10 30 -10 curveto
  30  0 translate
} def

/mwg-wave-p4 {
  10 -10 20 -5 30 0 curveto
  30  0 translate
} def

/mwg-wave-repeat {
  0.5 1 scale
  1 1 3 2 roll {
    4 mod
    dup 1 eq {
      mwg-wave-p1
    } {
      dup 2 eq {
        mwg-wave-p2
      } {
        dup 3 eq {
          mwg-wave-p3
        } {
          mwg-wave-p4
        } ifelse
      } ifelse
    } ifelse
    pop
  } for
  2 1 scale
} def

/mwg-wave-rel {
  gsave
    % [x y rx ry n w]
    6 4 roll translate
    0 0 moveto
    % [rx ry n w]
    4 2 roll 2 copy exch atan rotate
    % [n w rx ry]
    dup mul exch dup mul add sqrt
    % [n w r]
    2 index 15 mul div exch
    % [n sx w]
    20 div
    % [n sx sy]
    2 copy scale
    3 2 roll mwg-wave-repeat
    1 exch div exch  1 exch div exch   scale
    stroke
  grestore
} def

%-------------------------------------------------------------------------------
%5 25 245 135 mwg-draw-rect

/M_A1 -25 def
/M_WIDTH 1.5 def

140 60 translate
0.5 setlinewidth
gsave
  << >> begin
    /mwg-option-arrow-head-scale 0.7 def
    -100 0 100 0 mwg-draw-arrow
  end
  
  M_WIDTH setlinewidth
  -8 -12 8 12 mwg-draw-rect
  65 0 translate
  -8 -12 8 12 mwg-draw-rect
grestore

gsave
  gsave
    180 rotate
    0 0 30 M_A1 0 arc stroke
  grestore
  
  M_WIDTH setlinewidth [5 4] 0 setdash
  gsave
    M_A1 rotate
    -130 0 0 0 mwg-draw-arrow
    -130 0 2 0 360 arc fill
    
    newpath
    0.5 setlinewidth [] 0 setdash
    -100 0 translate -10 rotate
    -1 1 scale
    gsave
      2 setlinewidth
      1 setgray
      1 -10 0 20 4 5 mwg-wave-rel
    grestore
    0 -10 0 20 4 5 mwg-wave-rel
    2 -10 0 20 4 5 mwg-wave-rel
  grestore
  
  0 0 58 0 mwg-draw-arrow
  0 0 48 24 mwg-draw-arrow
  0 0 50 -18 mwg-draw-arrow
grestore

gsave
  10 mwg-font-mincho
  -72 36 moveto (\245\254\245\363\245\336\300\376) show % gamma sen
  30 30 moveto (\245\254\245\363\245\336\300\376) show % gamma sen
  -18 -25 moveto (\270\241\275\320\264\357) show % kenshutuki
  gsave 10 mwg-font-roman (A) show grestore
  52 -25 moveto (\270\241\275\320\264\357) show % kenshutuki
  gsave 10 mwg-font-roman (B) show grestore
  -130 62 moveto (\245\254\245\363\245\336\300\376\270\273) show % gamma sengen
  gsave 10 mwg-font-roman (S) show grestore
  
  -35 25 moveto
  10 mwg-font-math (E) show
  6 mwg-font-isym
  0 -1 rmoveto (g) show
  
  10 mwg-font-isym
  -40 4 moveto (q) show % theta
  
  10 mwg-font-math
  95 -10 moveto (x) show
grestore

