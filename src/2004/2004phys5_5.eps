%: <vec> <scal> vmul <vec>
/mwg-v2mul {
  exch aload pop
  %: m x y
  2 index mul
  exch  2 index mul exch
  %: m m*x m*y
  2 array astore
  exch pop
} def

%: <vec> <scal> mwg-v2div <vec>
/mwg-v2div {
  1 exch div mwg-v2mul
} def

/mwg-v2add {
  aload pop
  3 2 roll aload pop
  %: x y x y
  exch 3 1 roll add
  %: x x y+y
  3 1 roll add exch
  %: x+x y+y
  2 array astore
} def

/mwg-v2sub {
  exch aload pop
  3 2 roll aload pop
  %: x y x y
  exch 3 1 roll sub
  %: x x y-y
  3 1 roll sub exch
  %: x-x y-y
  2 array astore
} def

/mwg-v2norm {
  dup mwg-v2abs mwg-v2div
} def

/mwg-v2abs {
  aload pop
  dup mul exch dup mul add sqrt
} def

%: <i0> <iM> mwg-path-curve-impl
/mwg-path-curve-impl {
  data 0 get aload pop moveto
  1 exch {
    /i exch def
    /p0 i 1 sub getpt def
    /p1 i       getpt def
    /p2 i 1 add getpt def
    /p3 i 2 add getpt def
    
    p2 p0 vsub mwg-v2norm
      p2 p1 vsub vabs 0.4 mul mwg-v2mul
      p1 vadd aload pop
      
    p1 p3 vsub mwg-v2norm
      p2 p1 vsub vabs 0.4 mul mwg-v2mul
      p2 vadd aload pop
    
    p2 aload pop
    
    curveto
  } for
} def

/mwg-path-curve {
  << >> begin
    /data exch def
    /len data length def
    /vsub {mwg-v2sub} def
    /vadd {mwg-v2add} def
    /vabs {mwg-v2abs} def
    /getpt {
      dup -1 eq {
        pop
        data 0 get data 1 get vsub
          data 0 get vadd
      }{
        dup len eq {
          pop
          data len 1 sub get data len 2 sub get vsub
            data len 1 sub get vadd
        }{
          data exch get
        } ifelse
      } ifelse
    } def
    0 len 2 sub mwg-path-curve-impl
    stroke
  end
} def

/mwg-path-ccurve {
  << >> begin
    /data exch def
    /len data length def
    
    /vsub {mwg-v2sub} def
    /vadd {mwg-v2add} def
    /vabs {mwg-v2abs} def
    /getpt {
      len add len mod
      data exch get
    } def
    0 len 1 sub mwg-path-curve-impl
    stroke
  end
} def

/mwg-draw-curve  {mwg-path-curve stroke} def
